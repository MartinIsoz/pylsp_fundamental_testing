FoamFile
{
    version  2.0;
    format   ascii;
    class    dictionary;
    location "system";
    object   controlDict;
}
deltaT            1;
writeControl      timeStep;
writeInterval     1;
writeFormat       ascii;
writePrecision    7;
writeCompression  off; //uncompressed;
timeFormat        general;
timePrecision     6;
runTimeModifiable true;
